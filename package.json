{
  "name": "datingapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "start-fresh": "npx react-native start --reset-cache",
    "test": "jest",
    "build-apk": "npx react-native run-android --mode=release",
    "signinreport": "cd android && ./gradlew signingReport",
    "pod": "cd ios && pod install && cd ..",
    "gradle-clean": "cd android && gradlew clean && cd ..",
    "pod-install": "cd ios && pod deintegrate && pod install && cd ..",
    "gradle-clean-mac": "cd android && ./gradlew clean && cd ..",
    "build-android": "cd android && gradlew clean && cd .. && react-native run-android",
    "release-buildAPK": "cd android && ./gradlew clean && ./gradlew assembleRelease && cd ..",
    "release-buildAPK-window": "cd android && gradlew clean && gradlew assembleRelease && cd ..",
    "release-buildAAB": "cd android && ./gradlew clean && ./gradlew bundleRelease && cd ..",
    "release-buildAAB-window": "cd android && gradlew clean && gradlew bundleRelease && cd ..",
    "remove-DerivedData": "rm -rf ~/Library/Developer/Xcode/DerivedData && cd ios && xcodebuild clean && cd ..",
    "gradle-permission": "cd android && chmod +x gradlew && cd ..",
    "debug-build": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/ && cd android && ./gradlew clean && ./gradlew assembleDebug && cd ..",
    "debug-build-window": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/ && cd android && gradlew clean && gradlew assembleDebug && cd ..",
    "android-log": "npx react-native log-android",
    "ios-log": "npx react-native log-ios"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/geolocation": "^3.4.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native/new-app-screen": "0.83.0",
    "@react-navigation/bottom-tabs": "^7.8.12",
    "@react-navigation/native": "^7.1.25",
    "@react-navigation/native-stack": "^7.8.6",
    "axios": "^1.13.2",
    "formik": "^2.4.9",
    "i18next": "^25.8.0",
    "lucide-react-native": "^0.561.0",
    "react": "19.2.0",
    "react-i18next": "^16.5.4",
    "react-native": "0.83.0",
    "react-native-actions-sheet": "^10.1.1",
    "react-native-animatable": "^1.4.0",
    "react-native-blob-util": "^0.19.11",
    "react-native-confetti-cannon": "^1.5.2",
    "react-native-country-picker-modal": "^2.0.0",
    "react-native-date-picker": "^5.0.13",
    "react-native-floating-action": "^1.22.0",
    "react-native-image-crop-picker": "^0.51.1",
    "react-native-image-picker": "^7.2.3",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-localize": "^3.6.1",
    "react-native-permissions": "^5.4.4",
    "react-native-safe-area-context": "^5.5.2",
    "react-native-screens": "^4.19.0",
    "react-native-svg": "^15.15.1",
    "react-native-toast-message": "^2.3.3",
    "react-native-vector-icons": "^10.3.0",
    "socket.io-client": "^4.8.3",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-transform-export-namespace-from": "^7.27.1",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.83.0",
    "@react-native/eslint-config": "0.83.0",
    "@react-native/metro-config": "0.83.0",
    "@react-native/typescript-config": "0.83.0",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.2.0",
    "@types/react-test-renderer": "^19.1.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.2.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20"
  }
}
